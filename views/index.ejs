<%-include('./partials/header',{data:data,algo:algo})%>
<link rel="icon" type="image/x-icon" href="/img/network.ico">
<style>
  body{background-color:antiquewhite;}
  #main{background-image: linear-gradient(270deg, gainsboro, #979db0) !important;}
  .data-container { display: flex; height:65vh;justify-content: space-around;}
  .data-link a:not([href="/Brute Force"]){text-decoration: underline;cursor: pointer;margin: 5px;}
  .data-link a[href="/Brute Force"]{color: black;margin: 5px;pointer-events: none;cursor: default;}
  .data-link a[href="/Brute Force"]::after{content:" (Worst)"}
  footer{text-align: center;}
</style>
<main>
<div id= main class="ui segment main">
  <h1>Data Structures & Algorithms Visualization</h1>
  <p>
    Fundamental knowledge of Computer Science.<br>
    All contents are referenced or derived from Wikipedia and Internet Reseach.
  </p>
  <div class="data-container">
    <div class="data-structure">
      <h2>Data Structure</h2>
      <ul>
      <%data.forEach((data)=> { %>
        <li class="data-link list-data"><strong><a href="/<%= data.name %>"><%= data.name %></a></strong></li>
      <% }); %>
      </ul>
    </div>
    <div class="data-algorithm">
      <h2>Algorithm</h2>
      <ul>
      <%algo.forEach((algo)=> { %>
        <li class="ui">
          <strong><%= algo.name %></strong> 
          <% if(algo.name==="Known Algorithms"){%>
            <p>(More to come)</p>
          <% }; %>  
          <ul>
            <%algo.sub.name.forEach((name)=>{%>
              <li class="data-link"><a href="/<%= name %>"><%= name %></a></li>
            <%});%>
          </ul>
          
        </li><br>
      <% }); %>
      </ul>
    </div>
  
  </div>
  
</div>

</main>

<footer>
  <%-include('./partials/footer.js')%>
</footer>

</body> </html>